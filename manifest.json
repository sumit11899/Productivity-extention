{
    "manifest_version":2,
    "name":"productivity management",
    "version":"0.1",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    
   
    "browser_action":{
        "default_icon":"productivity.png",
        "default_popup":"index.html",
        "default_title":"productivity management"
    },
    "permissions":[
        "storage", "tabs", "notifications", "alarms"
    ]
}